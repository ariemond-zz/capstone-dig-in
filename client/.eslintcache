\n            draggable={false}\r\n            showDots={true}\r\n            responsive={responsive}\r\n            ssr={true} \r\n            infinite={true}\r\n            autoPlay={false}\r\n            autoPlaySpeed={1000}\r\n            keyBoardControl={true}\r\n            customTransition=\"all .5\"\r\n            transitionDuration={500}\r\n            containerClass=\"carousel-container\"\r\n            removeArrowOnDeviceType={[\"tablet\", \"mobile\", \"desktop\"]}\r\n            dotListClass=\"custom-dot-list-style\"\r\n            itemClass=\"carousel-item-padding-40-px\">\r\n\r\n            {reviews.map(review => <p className=\"reviews__single\">{review.reviews}</p>)} \r\n\r\n        </Carousel>\r\n      </div>\r\n      <div className=\"reviews__form-section\">\r\n        <form className=\"reviews__form\" onSubmit={reviewSubmit}>\r\n          <label htmlFor=\"name\" className=\"reviews__form-label\">Add a Review:</label>\r\n          <textarea \r\n            className=\"reviews__form-review-input\"\r\n            name=\"review\" \r\n            id=\"review\" \r\n            rows=\"5\"\r\n            wrap=\"hard\"\r\n            placeholder=\"Write review here\"\r\n            ref={review}\r\n            required></textarea>\r\n          <button className=\"reviews__form-button\" type=\"submit\">SUBMIT</button>\r\n          </form>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\n\r\nexport default Reviews","C:\\Users\\arian\\OneDrive\\Documents\\BrainStation\\dig-in-app\\client\\src\\components\\MessageModal\\MessageModal.jsx",["73"],"C:\\Users\\arian\\OneDrive\\Documents\\BrainStation\\dig-in-app\\client\\src\\components\\EditProfile\\EditProfile.jsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":11,"column":9,"nodeType":"80","messageId":"81","endLine":11,"endColumn":13},{"ruleId":"82","severity":1,"message":"83","line":43,"column":8,"nodeType":"84","endLine":43,"endColu